  "version": "0.0.1",
  "main": "index.html",
  "dependencies": {
    "co": "~3.0.6",
    "thunkify": "~2.1.1",
    "co-fs": "~1.2.0",
    "request": "~2.36.0",
    "clone": "~0.1.15",
    "underscore": "~1.6.0",
    "coffee-script": "~1.7.1",
    "shortid": "~2.0.0",
    "fs-extra": "~0.9.1",
    "mkdirp": "~0.5.0",
